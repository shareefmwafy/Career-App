{
  "expo": {
    "name": "Mobile",
    "scheme": "myapp",
    "slug": "career",
    "version": "1.0.0",
    "permissions": [
      "LOCATION"
    ],
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.career.project",
      "config": {
        "googleMapsApiKey": "AIzaSyAFRRxD0w9k4pdQ4PYsnxHGBGa_GbVYljU"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.career.mobile",
      "googleMaps": {
        "apiKey": "AIzaSyAFRRxD0w9k4pdQ4PYsnxHGBGa_GbVYljU"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-asset",
      "expo-font",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Kavoon-Regular.ttf"
          ]
        }
      ],
      "expo-location",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.155720957457-buoh0r927fjprq0j9pf9cckbof2gu2g2"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "894020d3-599f-4b42-921c-20e69adaba8b"
      }
    }
  }
}
